<script src="https://ajax.googleapis.com/ajax/libs/jquery/3.7.1/jquery.min.js"></script>

<script>
(function($) {
  'use strict';

  // Variant Tabs Manager
  class VariantTabsManager {
    constructor() {
      this.initialized = false;
      this.init();
    }

    init() {
      if (this.initialized) return;
      
      this.bindEvents();
      this.initializeTabs();
      this.initialized = true;
    }

    bindEvents() {
      // Use event delegation for tab clicks
      $(document).on('click', '.variant-tabs-container .variant-tab:not(.disabled)', (e) => {
        this.handleTabClick(e);
      });

      // Sync with original radio inputs
      $(document).on('change', 'input[type="radio"][name*="-"]', (e) => {
        this.handleRadioChange(e);
      });
    }

    handleTabClick(e) {
      const $tab = $(e.currentTarget);
      const optionPosition = $tab.data('option-position');
      const optionValue = $tab.data('option-value');
      
      // Update all tabs for this option
      this.updateTabsForOption(optionPosition, optionValue);
      
      // Find and trigger the corresponding radio input
      const $radioInput = $(`input[type="radio"][name*="-${optionPosition}"][value="${optionValue}"]`);
      if ($radioInput.length && !$radioInput.hasClass('disabled')) {
        $radioInput.prop('checked', true).trigger('change');
        
        // Trigger variant change event if needed
        $(document).trigger('variant:change', [$radioInput.data('variant-id')]);
      }
    }

    handleRadioChange(e) {
      const $radio = $(e.currentTarget);
      const optionName = $radio.attr('name');
      const optionPosition = optionName.split('-').pop();
      const optionValue = $radio.val();
      
      // Update tabs to match radio selection
      this.updateTabsForOption(optionPosition, optionValue);
    }

    updateTabsForOption(optionPosition, optionValue) {
      // Remove active class from all tabs in this option group
      $(`.variant-tab[data-option-position="${optionPosition}"]`).removeClass('active');
      
      // Add active class to selected tab
      $(`.variant-tab[data-option-position="${optionPosition}"][data-option-value="${optionValue}"]`).addClass('active');
      
      // Update option name display if exists
      const $optionName = $(`.original-variant-fieldset .form-label strong`);
      if ($optionName.length) {
        // You might want to update this based on your specific structure
        console.log('Option selected:', optionValue);
      }
    }

    initializeTabs() {
      // Initialize tabs based on current radio selections
      $('input[type="radio"][name*="-"]:checked').each((index, radio) => {
        const $radio = $(radio);
        const optionName = $radio.attr('name');
        const optionPosition = optionName.split('-').pop();
        const optionValue = $radio.val();
        
        this.updateTabsForOption(optionPosition, optionValue);
      });
    }

    // Public method to refresh if needed
    refresh() {
      this.initializeTabs();
    }
  }

  // Initialize when document is ready
  $(document).ready(() => {
    // Wait a bit to ensure all elements are loaded
    setTimeout(() => {
      window.variantTabsManager = new VariantTabsManager();
    }, 100);
  });

  // Reinitialize when section reloads (for Shopify sections)
  $(document).on('shopify:section:load', () => {
    if (window.variantTabsManager) {
      window.variantTabsManager.refresh();
    } else {
      window.variantTabsManager = new VariantTabsManager();
    }
  });

})(jQuery);
</script>





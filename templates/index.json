            "text_color": ""
          },
          "blocks": {
            "heading_XEnyCf": {
              "type": "heading",
              "settings": {
                "heading": "Every strand tells a story.",
                "heading_type": "h2",
                "heading_size": "md",
                "text_align": "start",
                "margin_bottom": 16
              },
              "blocks": {}
            },
            "text_h7wh8i": {
              "type": "text",
              "settings": {
                "text": "<p>What’s happening inside your body can show up in your hair long before you realise. It’s all connected, and understanding the root causes is the first step.</p>",
                "add_margin": false,
                "text_align": "start",
                "use_icons_in_list": false,
                "margin_bottom": 5
